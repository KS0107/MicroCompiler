// test_multiplication_division.micro
var a = 4 * (3 + 2);         // a = 4 * 5 = 20 (parentheses first)
var b = a / 4;               // b = 20 / 4 = 5
var c = (b + 2) * 3;         // c = (5 + 2) * 3 = 7 * 3 = 21
var d = 12 / (2 * 3);        // d = 12 / 6 = 2
var e = (a / b) * (c - d);   // e = (20 / 5) * (21 - 2) = 4 * 19 = 76
exit(e);                    // Should exit with e = 76